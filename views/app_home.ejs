<div class="nhc-global-container">

    <div class="nhc-container container-fluid">
        <ng-view></ng-view>
    </div>

    <div class="nhc-actionbar-spacer"></div>

    <div class="tool-bar">
        <div class="floating-toggle" ng-controller="FullscreenController">
            <div ng-if="fullscreenEnabled"
                 style="width: 64px"
                 class="tool-bar-item">
                <div draggable-toggle="{click: false, height: 32, handleWidth: 32, on: true, text: {on: '', off: ''}}"
                     class="round"
                     bounce-back="false"
                     click-off="toggleFullscreen()"
                     click-off-bounce-back="true"
                     on-class="fa fa-fw fa-angle-double-left"
                     off-class="fa fa-fw fa-dot-circle-o nhc-clickable"
                     handle-class="fa fa-fw fa-arrows-alt">
                </div>
            </div>
        </div>
