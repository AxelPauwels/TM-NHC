    </div>

    <div class="nhc-bar text-shadow-dark"
         ng-controller="NhcActionBarController"
         ng-disabled="locked">
        <div ng-repeat="action in actions | filter:{active:true}"
             class="nhc-bar-item nhc-clickable" ng-click="execute(action)">
            <img ng-if="action.imageUrl" ng-src="{{action.imageUrl}}"
                 class="nhc-action-image img-responsive img-rounded"/>
            <div class="nhc-action-title">
                <span ng-if="action.icon" class="fa fa-fw"
                      ng-class="action.icon"></span>
                <span ng-if="action.title" ng-bind="action.title"></span>
            </div>
        </div>
        <div class="nhc-bar-item" ng-controller="SpinnerController">
            <spinner name="nhcActionBarSpinner" class="nhc-clickable"
                     ng-click="resetSpinner($event)">
                <div class="fa fa-spin fa-spinner" aria-hidden="true"></div>
                <div>Even geduld <br>a.u.b...</div>
                <div class="bouncing-bar-spinner"></div>
            </spinner>
        </div>
        <div class="nhc-bar-item nhc-bar-logo">
            <img class="nhc-action-image"
                 src="/images/logo_natuurhulpcentrum_white.svg"/>
            <div class="nhc-action-title">
                <span>Natuurhulpcentrum</span>
            </div>
            <div class="floating-toggle">
                <div style="width: 64px">
                    <div draggable-toggle="{click: false, height: 32, handleWidth: 32, on: true, text: {on: '', off: ''}}"
                         do-toggle="toggleActionBarLock()"
                         bounce-back="true"
                         class="round"
                         on-class="fa fa-fw fa-angle-double-left"
                         handle-class="fa fa-fw fa-lock">
                    </div>
                </div>
            </div>
            <div class="screensize"
                 ng-bind="windowInnerWidth + ' x ' + windowInnerHeight"></div>
        </div>
        <div class="nhc-bar-item nhc-clickable nhc-bar-clock"
             ng-controller="ClockController" ng-click="executeAction()">
            <ds-widget-clock show-analog theme="nhc-light"
                             class="nhc-action-image img-responsive img-rounded"></ds-widget-clock>
            <ds-widget-clock show-digital
                             digital-format="clock_format"
                             class="nhc-action-title"></ds-widget-clock>
        </div>
    </div>

